{
  "manifestVersion": 1,
  "id": "AzureDnsZoneManager",
  "name": "Azure DNS zone manager",
  "version": "__Build.BuildNumber__",
  "publisher": "experta",
  "targets": [
      {
          "id": "Microsoft.VisualStudio.Services"
      }
  ],    
  "description": "Tasks package for Azure DNS Zone",
  "tags": [
    "Azure DNS Zone",
    "Build",
    "Release",
    "Azure Pipelines",
    "macOS",
    "linux",
    "windows"
  ],
  "links":{
      "support": { 
          "uri": "https://github.com/lturmel/AzureDNSZoneExtensions/issues"
      }
  },
  "repository": {
    "type" : "git",
    "uri" : "https://github.com/lturmel/AzureDNSZoneExtensions"
  },
  "categories": [
      "Azure Pipelines"
  ],
  "icons": {
      "default": "icon.png"        
  },
  "content": {
    "details": {
      "path": "readme.md"
    },
    "license": {
      "path": "license"
    }
  },
  "files": [
      {
        "path": "aRecordManager"
      }
  ],
  "public": true,
  "contributions": [
      {
          "id": "custom-build-release-task",
          "type": "ms.vss-distributed-task.task",
          "targets": [
              "ms.vss-distributed-task.tasks"
          ],
          "properties": {
              "name": "aRecordManager"
          }
      }
  ]
}